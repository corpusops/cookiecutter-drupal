{

    "name": "{{cookiecutter.git_ns}}/{{cookiecutter.git_project}}",
    "description": "{{ cookiecutter.name }}",
    "type": "project",
    "license": "proprietary",
    "authors": [
        {
            "name": "fixme",
            "email": "fixme@example.com",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "git",
            "url": "https://github.com/previousnext/drush_cmi_tools.git"
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "1.6.2",
        "drupal/big_pipe_sessionless": "1.1",
        "drupal/cdn": "3.2",
        "drupal/content_lock": "1.0-alpha8",
        "drupal/content_moderation_notifications": "3.0-rc2",
        "drupal/core": "8.6.7",
        "drupal/ctools": "3.0",
        "drupal/diff": "1.0-rc1",
        "drupal/drush-cmi-tools": "dev-8.x-1.x",
        "drupal/editor_advanced_link": "1.4",
        "drupal/editor_file": "1.2",
        "drupal/entity_browser": "2.0-alpha2",
        "drupal/entity_embed": "1.0-beta2",
        "drupal/eu_cookie_compliance": "1.1",
        "drupal/flood_unblock": "1.5",
        "drupal/google_analytics": "2.2",
        "drupal/google_tag": "1.0",
        "drupal/honeypot": "1.27",
        "drupal/inline_entity_form": "1.0-beta1",
        "drupal/keycloak": "1.3",
        "drupal/linkit": "5.0-beta7",
        "drupal/metatag": "1.5",
        "drupal/pardot": "1.x-dev",
        "drupal/pathauto": "1.1",
        "drupal/redirect": "1.1",
        {%if cookiecutter.with_redis%}"drupal/redis": "1.0",{% endif %}
        "drupal/schema_metatag": "1.2",
        "drupal/simple_sitemap": "2.11",
        "drupal/sitemap": "1.3",
        "drupal/ultimate_cron": "2.0-alpha3",
        "drupal/webform": "5.0-rc8",
        "drupal/workbench_access": "1.0-beta1",
        "drupal/workflow_participants": "2.3",
        "drupal-composer/drupal-scaffold": "^2.2",
        "drush/drush": "8.1.16",
        "elasticsearch/elasticsearch": "~5.0",
        "roave/security-advisories": "dev-master",
        "webflo/drupal-finder": "^1.0",
        "webmozart/path-util": "^2.3",
        "wikimedia/composer-merge-plugin": "^1.4",
        "drupal/console": "1.7.0"
    },
    "require-dev": {
        "behat/mink": "~1.7",
        "behat/mink-goutte-driver": "~1.2",
        "jcalderonzumba/gastonjs": "~1.0.2",
        "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
        "mikey179/vfsstream": "~1.2",
        "phpunit/phpunit": ">=4.8.28 <5",
        "symfony/css-selector": "~2.8",
        "drupal/drupal-extension": "~3.3.0",
        "behat/behat": "^3.0",
        "drupal/coder": "~8.2.0",
        "guzzlehttp/guzzle" : "^6.0@dev",
        "drupal/devel": "1.2"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "process-timeout": 0
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "installer-paths": {
            "www/core": ["type:drupal-core"],
            "www/libraries/{$name}": ["type:drupal-library"],
            "www/modules/{$name}": ["type:drupal-module"],
            "www/profiles/{$name}": ["type:drupal-profile"],
            "www/themes/{$name}": ["type:drupal-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"]
        },
        "patches": {
        }
    }
}
