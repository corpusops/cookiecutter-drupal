{
    "name": "myapp",
    "php_ver": "8.0",
    "py_ver": "3.8",
    "compose_version": "3.8",
    "composer_ver": "2",
    "base_os": "ubuntu",
    "ssl_in_dev": "1",
    "nginx_in_dev": "y",
    "maintenance_no_503": "",
    "git_ns": "{{cookiecutter.name}}-group",
    "lname": "{{cookiecutter.name|lower}}",
    "profile_name": "{{cookiecutter.lname.replace('-','')}}",
    "out_dir": ".",
    "tld_domain": "example.com",
    "app_type": "drupal",
    "use_submodule_for_deploy_code": "y",

    "local_domain": "{{cookiecutter.lname}}.local",
    "local_http_port": "8008",
    "local_https_port": "8448",
    "local_direct_db_port": "8545",
    "dev_domain": "dev-{{cookiecutter.lname}}.{{cookiecutter.tld_domain}}",
    "qa_domain": "qa-{{cookiecutter.lname}}.{{cookiecutter.tld_domain}}",
    "staging_domain": "staging-{{cookiecutter.lname}}.{{cookiecutter.tld_domain}}",
    "prod_domain": "{{cookiecutter.lname}}.{{cookiecutter.tld_domain}}",
    "preprod_domain": "{{cookiecutter.lname}}.{{cookiecutter.tld_domain}}",
    "dev_alternate_domains": "www.{{cookiecutter.dev_domain}}",
    "qa_alternate_domains": "www.{{cookiecutter.qa_domain}}",
    "staging_alternate_domains": "www.{{cookiecutter.staging_domain}}",
    "prod_alternate_domains": "www.{{cookiecutter.prod_domain}}",
    "preprod_alternate_domains": "www.{{cookiecutter.prod_domain}}",

    "site_name": "{{cookiecutter.lname}}",
    "staging_host": "{{cookiecutter.staging_domain.replace('-{0}.'.format(cookiecutter.lname), '-docker-{0}.'.format(cookiecutter.lname))}}",
    "qa_host": "{{cookiecutter.qa_domain.replace('-{0}.'.format(cookiecutter.lname), '-docker-{0}.'.format(cookiecutter.lname))}}",
    "dev_host": "{{cookiecutter.dev_domain.replace('-{0}.'.format(cookiecutter.lname), '-docker-{0}.'.format(cookiecutter.lname))}}",
    "preprod_host": "preprod-{{cookiecutter.preprod_domain.replace('-{0}.'.format(cookiecutter.lname), '-docker-{0}.'.format(cookiecutter.lname))}}",
    "prod_host": "{{cookiecutter.prod_domain.replace('-{0}.'.format(cookiecutter.lname), '-docker-{0}.'.format(cookiecutter.lname))}}",
    "staging_port": "22",
    "remove_fg": "1",
    "remove_doc": "",
    "remove_cron": "",
    "enable_cron": "",
    "remove_varnish": "1",
    "qa_port": "22",
    "dev_port": "22",
    "drupal_project_name": "{{cookiecutter.lname}}",
    "preprod_port": "22",
    "prod_port": "22",
    "app_path": "/code/app/",
    "drupal_env": "/code/app/.env",
    "drupal_settings": "/code/app/www/sites/default/settings.php",
    "public_path": "/code/app/www",
    "public_files_path": "/code/app/var/public",
    "private_path": "/code/app/var/private",
    "cache_path": "/code/app/var/cache/",
    "bin_path": "/code/app/bin/",
    "mail_domain": "{{cookiecutter.tld_domain }}",
    "infra_domain": "{{cookiecutter.tld_domain }}",
    "git_server": "gitlab.{{cookiecutter.infra_domain}}",
    "git_project_server": "{{cookiecutter.git_server}}",
    "git_scheme": "https",
    "git_user": "",

    "test_tests": "y",
    "test_e2e": "y",
    "test_cypress": "y",
    "test_linting": "",
    "test_coverage": "",
    "haproxy": "",
    "git_url": "{{cookiecutter.git_scheme}}://{%if cookiecutter.git_user%}{{cookiecutter.git_user}}@{%endif%}{{cookiecutter.git_project_server}}",
    "git_project": "{{cookiecutter.lname}}",
    "git_project_url": "{{cookiecutter.git_scheme}}://{%if cookiecutter.git_user%}{{cookiecutter.git_user}}@{%endif%}{{cookiecutter.git_project_server}}/{{cookiecutter.git_ns}}/{{cookiecutter.git_project}}",
    "fname_slug": "{{'{0}-{1}'.format(cookiecutter.git_ns, cookiecutter.lname)|lower|replace('/', '-')|replace('_', '-')}}",
    "runner_tag": "{{cookiecutter.fname_slug}}-ci",
    "deploy_project_url": "https://github.com/corpusops/{{cookiecutter.app_type}}-deploy-common.git",
    "deploy_project_dir": "local/{{cookiecutter.deploy_project_url.split('/')[-1].replace('.git', '')}}",
    "with_sentry": "",
    "with_redis": "",
    "docker_registry": "registry.{{cookiecutter.infra_domain}}",
    "registry_is_gitlab_registry": "{% if 'gitlab.com' in cookiecutter.docker_registry%}y{%endif%}",
    "docker_registry_group": "{{cookiecutter.git_ns.split('/')[0].lower()}}",
    "simple_docker_image": "{{cookiecutter.docker_registry_group}}/{{cookiecutter.lname}}",
    "simple_docker_image_varnish": "{{cookiecutter.docker_registry_group}}/{{cookiecutter.lname}}-varnish",
    "docker_image": "{%if cookiecutter.docker_registry%}{{cookiecutter.docker_registry}}/{%endif%}{{cookiecutter.simple_docker_image}}",
    "db_mode": "postgresql",
    "db_version": "10",
    "tz": "Europe/Paris",
    "db_port": "{% if cookiecutter.db_mode.startswith('m') %}3306{%else%}5432{%endif%}",
    "use_i18n": "y",
    "use_l10n": "y",
    "registry_user": "{%if cookiecutter.registry_is_gitlab_registry%}gitlab-ci-token{%endif%}",
    "registry_password": "{%if cookiecutter.registry_is_gitlab_registry%}$CI_JOB_TOKEN{%endif%}",
    "statics_uri": "/sites/default/files",
    "use_tz": "y",
    "language_code": "fr-fr",
    "postgis_image": "corpusops/pgrouting:10.1-2.5.4",
    "postgresql_image": "corpusops/postgres:{{cookiecutter.db_version}}",
    "postgres_image": "{{cookiecutter.postgresql_image}}",
    "mysql_image": "corpusops/mariadb:10.6",
    "redis_image": "corpusops/redis:4.0-alpine",
    "nginx_image": "corpusops/project:nginx-stable-alpine",
    "user_model": "",
    "php_user": "{{cookiecutter.app_type}}",
    "php_group": "{%if cookiecutter.base_os=='centos'%}apache{%else%}www-data{%endif%}",
    "cache_system": "redis",
    "cache_image": "{{cookiecutter['{0}_image'.format(cookiecutter.cache_system)]}}",
    "with_yarn": "",
    "with_php_status": "y",
    "php_status": "/php-status-drupal",
    "php_memory_limit": "128M",
    "with_nginx_status": "y",
    "nginx_status": "/nginx-status",
    "no_cache_cookie": "NO_CACHE",
    "no_cache_url": "/foo/bar/",
    "dev_branch": "dev",
    "preprod_branch": "preprod",
    "qa_branch": "qa",
    "staging_branch": "staging",
    "prod_branch": "prod",
    "debian_image": "debian:11-slim",
    "ubuntu_image": "ubuntu:22.04",
    "centos_image": "centos:7",
    "helpers_debian_image": "corpusops/debian:11-slim",
    "helpers_ubuntu_image": "corpusops/ubuntu-bare:22.04",
    "helpers_centos_image": "corpusops/centos-bare:7",
    "base_image": "{{cookiecutter['{0}_image'.format(cookiecutter.base_os)]}}",
    "helpers_image": "{{cookiecutter['helpers_{0}_image'.format(cookiecutter.base_os)]}}",
    "mailhog_image": "corpusops/project:mailhog",
    "varnish_image": "{{cookiecutter.ubuntu_image}}",
    "docs_image": "{{cookiecutter.ubuntu_image}}",
    "registry_image": "corpusops/gitlabtools:registryproxy",
    "compose_image": "corpusops/gitlabtools:compose",
    "docker_test_image": "busybox",
    "dind_image": "corpusops/gitlabtools:dind",
    "rsyslog_image": "corpusops/rsyslog",
    "syslog_port": "10514",
    "deploy_playbook": ".ansible/playbooks/app.yml",
    "with_bundled_docs": "",
    "with_bundled_front": "",
    "with_teleport_stagingprod_jobs": "",
    "node_version": "14.18.0",
    "node_image": "corpusops/node",
    "corpusops_image": "corpusops/ubuntu:20.04",
    "docs_folder": "app/var/docs",
    "docs_folders": "app/var/doc*",
    "statics_folders": "app/www/profiles/*/themes/*/dist",
    "node_image_flavor": "-slim",
    "with_reset_stagingprod_jobs": "",
    "main_branch": "master",
    "monitoring_network": "10.0.0.0/8",
    "sphinx_theme": "sphinx_makina_theme",
    "sphinx_theme_path": "",
    "sphinx_copyright": "FIXME",
    "sphinx_author": "FIXME",
    "no_tags_pipelines": "y"
}
